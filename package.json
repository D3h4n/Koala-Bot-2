{
  "name": "koala-bot-2",
  "version": "0.0.0",
  "description": "Upgraded bot, specifically designed for AKK",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D3h4n/Koala-Bot-2.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Automation"
  ],
  "author": "D3h4n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/D3h4n/Koala-Bot-2/issues"
  },
  "homepage": "https://github.com/D3h4n/Koala-Bot-2#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-jest": "^27.2.3",
    "fast-check": "^3.12.0",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@distube/soundcloud": "^1.3.3",
    "@distube/spotify": "^1.5.1",
    "discord.js": "^14.13.0",
    "distube": "^4.1.0",
    "dotenv": "^16.3.1",
    "sodium": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --collectCoverage --",
    "register": "ts-node  --transpile-only -r tsconfig-paths/register src/register.ts --",
    "watch": "nodemon -e ts -w src -x \"npm run start:dev\"",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "npm run start:dev -- --transpile-only",
    "clean": "rm -rf reports coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
