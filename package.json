{
  "name": "koala-bot-2",
  "version": "0.0.0",
  "description": "Upgraded bot, specifically designed for AKK",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "jest --ci --reporters=default --reporters=jest-junit --",
    "coverage": "jest --collectCoverage --ci --reporters=default --reporters=jest-junit --",
    "build": "tsc --removeComments --strict --pretty",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D3h4n/Koala-Bot-2.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Automation"
  ],
  "author": "D3h4n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/D3h4n/Koala-Bot-2/issues"
  },
  "homepage": "https://github.com/D3h4n/Koala-Bot-2#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-jest": "^27.1.2",
    "jest": "^29.2.0",
    "jest-junit": "^14.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3"
  },
  "jest": {
    "testPathIgnorePatterns": ["dist/*"],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
