{
  "name": "koala-bot-2",
  "version": "0.0.0",
  "description": "Upgraded bot, specifically designed for AKK",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --collectCoverage",
    "watch": "tsc-watch --onsuccess \"npm run start\"",
    "build": "tsc --removeComments --strict --pretty",
    "start": "node dist/index.js --",
    "preregister": "npm run build",
    "register": "node dist/register.js --",
    "clean": "rm -rf dist reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D3h4n/Koala-Bot-2.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Automation"
  ],
  "author": "D3h4n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/D3h4n/Koala-Bot-2/issues"
  },
  "homepage": "https://github.com/D3h4n/Koala-Bot-2#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-jest": "^27.2.3",
    "fast-check": "^3.12.0",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@distube/ytdl-core": "^4.11.17",
    "discord.js": "^14.13.0",
    "distube": "^4.0.6",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "sodium": "^3.0.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/*"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
