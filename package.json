{
  "name": "koala-bot-2",
  "version": "0.0.0",
  "description": "Upgraded bot, specifically designed for AKK",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --collectCoverage",
    "watch": "tsc-watch --onsuccess \"npm run start\"",
    "build": "tsc --removeComments --strict --pretty",
    "start": "node dist/index.js --",
    "preregister": "npm run build",
    "register": "node dist/register.js --",
    "clean": "rm -rf dist reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D3h4n/Koala-Bot-2.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Automation"
  ],
  "author": "D3h4n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/D3h4n/Koala-Bot-2/issues"
  },
  "homepage": "https://github.com/D3h4n/Koala-Bot-2#readme",
  "devDependencies": {
    "@discordjs/rest": "^1.5.0",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-check": "^3.2.0",
    "jest": "^29.2.0",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@distube/ytdl-core": "^4.11.7",
    "discord.js": "^14.6.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^4.4.1",
    "sodium": "^3.0.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/*"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
